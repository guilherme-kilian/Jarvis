@inject NavigationManager _navManager

<div class="d-flex gap-2 flex-column mb-4">
    <FluentIcon OnClick="@(() => _navManager.NavigateTo("/"))" Value="@(new Microsoft.FluentUI.AspNetCore.Components.Icons.Regular.Size20.ArrowLeft())" />
    <h4>@Title</h4>
</div>

@code {
    [Parameter, EditorRequired]
    public string Title { get; set; } = string.Empty;
}
